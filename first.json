{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://raw.githubusercontent.com/paristhecity/understanding/master/data/drugs.csv",
    "format": {
      "type": "csv"
    }
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "bin": true,
      "field": "Year",
      "type": "temporal"
    },
    "y": {
      "field": "Year",
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}